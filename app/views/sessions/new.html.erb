<h1>Sign in</h1>

<form action=" <%= session_url %>" method="post">
  <%= auth_token %>
  <label>Email <input type="text" name="session[email]" value="<%= @user ? @user.email : nil %>"></label>
  <br>
  <label>Password <input type="password" name="session[password]" value=""></label>
  <br>
  <input type="submit" value="Sign in">
</form>
