<h1>Sign up!</h1>

<form action=" <%= users_url %>" method="post">
  <%= auth_token %>
  <label>Email <input type="text" name="user[email]" value="<%= @user.email %>"></label>
  <br>
  <label>Password <input type="password" name="user[password]" value=""></label>
  <br>
  <input type="submit" value="Sign up">
</form>
